// prisma/schema.prisma

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") 
}

generator client {
  provider = "prisma-client-js"
}

model CreditRequest {
  id        String   @id @default(uuid())
  userId    String
  amount    Float
  status    String
  createdAt DateTime @default(now())
}
